{
    "version": 2,

    "builds": [
       { "src": "./index.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)",
        "dest": "/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        }
    ],
    "headers": [
        {
         "sounce"："/(.*)”,
         "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      },
        {
          "source": "/vercel_app_domain_name/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value":"*"}
          ]
       }
    ]
}
